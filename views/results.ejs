<h1>Results Page</h1>

<!-- Running forEach loop on "data".Search , data defined in app.js as parsed API data -->
<ul>
    <% data.Search.forEach(function(movie){ %>
        <li><strong><%= movie.Title %></strong> &mdash; <%= movie.Year %></li>
    <% }) %>
</ul>

<a href="/">Search for new movies</a>
